{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "APP",
  "extends": "./base.json",
  "compilerOptions": {
    "isolatedModules": false,
    "esModuleInterop": true,
    "allowJs": false,
    "resolveJsonModule": true,
    "noEmit": false,
    "jsx": "preserve",
    "target": "es2019",
    "lib": ["es2021", "dom", "esnext"]
  }
}
