{
  "name": "ui",
  "version": "0.0.0",
  "dependencies": {
    "@tamagui/animations-css": "1.39.7",
    "@tamagui/animations-react-native": "1.39.7",
    "@tamagui/font-inter": "1.39.7",
    "@tamagui/lucide-icons": "1.39.7",
    "@tamagui/react-native-media-driver": "1.39.7",
    "@tamagui/shorthands": "1.39.7",
    "@tamagui/theme-base": "1.39.7",
    "expo-font": "11.1.1",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-native-svg": "13.9.0",
    "solito": "^2.1.3"
  },
  "devDependencies": {
    "@tamagui/build": "1.39.7",
    "camelcase": "^6.0.0",
    "cheerio": "^1.0.0-rc.5",
    "esbuild-register": "^3.4.2",
    "eslint": "8.44.0",
    "fs-extra": "^11.1.1",
    "prettier": "latest",
    "typescript": "4.9.4",
    "uppercamelcase": "^3.0.0"
  },
  "files": [
    "types",
    "dist"
  ],
  "main": "./src/index.ts",
  "module:jsx": "src",
  "private": true,
  "scripts": {
    "build": "tamagui-build --ignore-base-url",
    "build:icons": "node -r esbuild-register ./src/scripts/componentize-icons.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc -b",
    "watch": "yarn build --watch"
  },
  "sideEffects": [
    "*.css"
  ],
  "types": "./types/index"
}
